{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      },
      "channel": "development"
    },
    "storybookDev": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "storybookDev"
      },
      "channel": "storybook-dev"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "preview"
      },
      "channel": "preview"
    },
    "storybookPreview": {
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "storybookDev"
      },
      "node": "20.5.1",
      "channel": "storybook-preview"
    },
    "production": {
      "channel": "production"
    }
  }
}
