{
  "name": "open-book-shelf",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "bunx expo start --dev-client",
    "start:debug": "EXPO_PUBLIC_LOG_LEVEL=debug bunx expo start --dev-client",
    "ios": "bunx react-native run-ios",
    "android": "bunx react-native run-android --active-arch-only",
    "test:detox": "detox test -c ios.sim.debug",
    "build:detox": "detox build -c ios.sim.debug",
    "ci:test:detox": "detox test -c ios.sim.release -l verbose --cleanup",
    "ci:build:detox": "detox build -c ios.sim.release",
    "compile": "tsc --noEmit -p . --pretty",
    "format": "biome format --write app test",
    "lint": "biome lint app test",
    "patch": "bunx patch-package",
    "test": "jest",
    "test:watch": "jest --watch",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "postinstall": "node ./bin/postInstall",
    "bundle:ios": "bunx react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "bundle:android": "bunx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "release:ios": "echo 'Not implemented yet: release:ios. Use Xcode. More info: https://reactnative.dev/docs/next/publishing-to-app-store'",
    "release:android": "cd android && rm -rf app/src/main/res/drawable-* && ./gradlew assembleRelease && cd - && echo 'APK generated in ./android/app/build/outputs/apk/release/app-release.apk'",
    "clean": "bunx react-native-clean-project",
    "clean-all": "bunx react-native clean-project-auto",
    "expo:start": "bunx expo start",
    "expo:android": "bunx expo start --android",
    "expo:ios": "bunx expo start --ios",
    "expo:web": "bunx expo start --web",
    "debug:web": "EXPO_PUBLIC_LOG_LEVEL=debug bunx expo start --web",
    "desktop": "tauri dev",
    "expo:build:detox": "detox build -c ios.sim.expo",
    "expo:test:detox": "./bin/downloadExpoApp.sh && detox test --configuration ios.sim.expo",
    "reactotron": "bun run adb && reactotron-app",
    "storybook-generate": "sb-rn-get-stories",
    "storybook-watch": "sb-rn-watcher",
    "storybook-native": "sb-rn-get-stories --config-path .storybook/native && STORYBOOK_ENABLED='true' && bunx expo start --dev-client",
    "storybook:web": "sb dev --config-dir .storybook/web -p 6006",
    "build-storybook": "build-storybook",
    "build:web": "bunx expo export --platform web --output-dir dist",
    "deploy:pages": "gh-pages -d dist -b gh-pages"
  },
  "overrides": {
    "react-error-overlay": "6.0.9",
    "jsonfile": "6.1.0"
  },
  "dependencies": {
    "@expo-google-fonts/noto-serif-jp": "^0.2.2",
    "@expo-google-fonts/space-grotesk": "^0.2.2",
    "@expo/config-plugins": "~7.8.0",
    "@expo/dev-server": "^0.5.5",
    "@expo/metro-config": "~0.17.1",
    "@expo/metro-runtime": "~3.1.3",
    "@expo/vector-icons": "^14.0.0",
    "@gluestack-style/react": "^1.0.49",
    "@gluestack-ui/config": "^1.1.16",
    "@gluestack-ui/themed": "^1.1.9",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/datetimepicker": "7.7.0",
    "@react-native-community/slider": "4.4.2",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@shopify/flash-list": "1.6.3",
    "apisauce": "^3.0.1",
    "date-fns": "^2.29.2",
    "expo": "~50.0.20",
    "expo-application": "~5.8.4",
    "expo-build-properties": "~0.11.1",
    "expo-constants": "~15.4.6",
    "expo-dev-client": "~3.3.12",
    "expo-device": "~5.9.4",
    "expo-document-picker": "~11.10.1",
    "expo-file-system": "~16.0.9",
    "expo-font": "~11.10.3",
    "expo-image": "~1.10.6",
    "expo-image-picker": "~14.7.1",
    "expo-localization": "~14.8.4",
    "expo-screen-orientation": "~6.4.1",
    "expo-sharing": "~11.10.0",
    "expo-splash-screen": "~0.26.5",
    "expo-status-bar": "~1.11.1",
    "expo-updates": "~0.24.13",
    "i18n-js": "3.9.2",
    "mobx": "6.6.0",
    "mobx-react-lite": "3.4.0",
    "mobx-state-tree": "5.1.5",
    "pdfjs-dist": "^4.10.38",
    "r2-opds-js": "^1.0.39",
    "r2-utils-js": "^1.0.32",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.39.1",
    "react-native": "0.73.6",
    "react-native-blob-util": "^0.18.6",
    "react-native-bootsplash": "4.3.2",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-modalfy": "^3.5.2",
    "react-native-pdf": "^6.7.1",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-web": "~0.19.6",
    "react-pdf": "^8.0.2",
    "transliteration": "^2.6.1",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-decorators": "7.18.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-optional-catch-binding": "7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-transform-arrow-functions": "^7.0.0",
    "@babel/plugin-transform-shorthand-properties": "^7.0.0",
    "@babel/plugin-transform-template-literals": "^7.0.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@biomejs/biome": "1.6.0",
    "@config-plugins/react-native-blob-util": "^6.0.0",
    "@config-plugins/react-native-pdf": "^6.0.0",
    "@storybook/addon-actions": "^7.6.16",
    "@storybook/addon-controls": "^7.6.16",
    "@storybook/addon-essentials": "^7.6.16",
    "@storybook/addon-interactions": "^7.6.16",
    "@storybook/addon-links": "^7.6.16",
    "@storybook/addon-ondevice-actions": "^7.6.15",
    "@storybook/addon-ondevice-backgrounds": "^7.6.15",
    "@storybook/addon-ondevice-controls": "^7.6.15",
    "@storybook/addon-ondevice-knobs": "^7.6.15",
    "@storybook/addon-ondevice-notes": "^7.6.15",
    "@storybook/addon-react-native-web": "^0.0.23",
    "@storybook/react": "^7.6.13",
    "@storybook/react-native": "^7.6.15",
    "@storybook/react-webpack5": "^7.6.13",
    "@storybook/testing-library": "^0.2.2",
    "@tauri-apps/cli": "^1.5.9",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/i18n-js": "3.8.2",
    "@types/jest": "26.0.19",
    "@types/react": "~18.2.45",
    "@types/xmldom": "^0.1.31",
    "babel-jest": "^29.2.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-react-docgen-typescript": "^1.5.1",
    "babel-plugin-react-native-web": "^0.19.7",
    "detox": "19.12.1",
    "detox-expo-helpers": "0.6.0",
    "expo-detox-hook": "1.0.10",
    "gh-pages": "^6.1.0",
    "http-proxy-middleware": "^2.0.6",
    "jest": "^29.3.1",
    "jest-expo": "^50.0.0",
    "mocha": "6",
    "patch-package": "6.4.7",
    "reactotron-core-client": "^2.8.13",
    "reactotron-mst": "^3.1.7",
    "reactotron-react-js": "^3.3.11",
    "reactotron-react-native": "^5.0.5",
    "storybook": "^7.6.16",
    "storybook-dark-mode": "^3.0.3",
    "ts-jest": "^29.1.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.3.0"
  },
  "detox": {
    "test-runner": "jest",
    "runnerConfig": "./detox/config.json",
    "specs": "detox",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/OpenBookShelf.app",
        "build": "xcodebuild -workspace ios/OpenBookShelf.xcworkspace -scheme OpenBookShelf -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build -destination 'name=iPhone 14'",
        "type": "ios.simulator",
        "device": {
          "name": "iPhone 14",
          "os": "iOS 15.5"
        }
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/OpenBookShelf.app",
        "build": "xcodebuild -workspace ios/OpenBookShelf.xcworkspace -scheme OpenBookShelf -configuration Release -sdk iphonesimulator -derivedDataPath ios/build -destination 'name=iPhone 14'",
        "type": "ios.simulator",
        "device": {
          "name": "iPhone 14",
          "os": "iOS 15.5"
        }
      },
      "ios.sim.expo": {
        "binaryPath": "bin/Exponent.app",
        "type": "ios.simulator",
        "name": "iPhone 14"
      }
    }
  }
}
